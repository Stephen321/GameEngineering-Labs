public class Consumer extends Thread {
	int[] b = new int[Lab2.n];

	@Override
	public void run() {
		while (Lab2.p < Lab2.n) {
			while (! (Lab2.p > Lab2.c));
			b[Lab2.c] = Lab2.buf;
			Lab2.c = Lab2.c + 1;
			System.out.println("c: " + Lab2.c +  " p: " + Lab2.p + " buf: " + Lab2.buf + " b: " + b );
		} 
		
	}
	
}
