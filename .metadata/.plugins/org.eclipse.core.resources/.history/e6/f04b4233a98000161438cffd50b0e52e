
public class PizzaFactory extends Thread {
	static int count = 0;
	
	@Override
	public void run() {
		while (true){
			int i = ++count;
//			try {
//				Thread.sleep(100);
//			} catch (InterruptedException e) {
//				e.printStackTrace();
//			}
			
			System.out.println("Pepperoni" + i);
		}
	}
}
